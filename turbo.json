{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [".env"],
    "tasks": {
        "build": {
            "inputs": ["$TURBO_DEFAULT$", ".env", ".env.*"],
            "outputs": [".next/**", "!.next/cache/**"]
        },
        "lint": {},
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}
